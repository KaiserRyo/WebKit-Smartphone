#!/bin/bash

pushd $OLYMPIAROOT/olympia

if [ -d "$OLYMPIAROOT/olympia/WebKitBuild/$CPUNAME/$OLYMPIABUILDCONFIG" ] ; then
    rm -rf "$OLYMPIAROOT/olympia/WebKitBuild/$CPUNAME/$OLYMPIABUILDCONFIG"
else
    echo "Can't clean: no WebKit build found in $OLYMPIAROOT/olympia/WebKitBuild/$CPUNAME/$OLYMPIABUILDCONFIG"
fi

popd
